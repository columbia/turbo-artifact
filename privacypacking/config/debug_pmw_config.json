{
    "omegaconf": {
        "epsilon": 10,
        "delta": 1e-05,
        "scheduler": {
            "metric_recomputation_period": 50,
            "scheduler_timeout_seconds": 1200,
            "data_lifetime": 0.1,
            "task_lifetime": 1,
            "planner": "PerBlockPlanner",
            "scheduling_wait_time": 1,
            "method": "batch",
            "metric": "Fcfs",
            "n": 1,
            "enable_caching": true
        },
        "metric": {
            "normalize_by": "available_budget",
            "n_knapsack_solvers": 1
        },
        "logs": {
            "verbose": false,
            "save": true
        },
        "blocks": {
            "initial_num": 1,
            "max_num": 400,
            "path": "covid19/covid19_data/blocks",
            "metadata": "covid19/covid19_data/metadata.json"
        },
        "tasks": {
            "sampling": "",
            "path": "covid19/covid19_workload/privacy_tasks.csv",
            "queries_path": "covid19/covid19_queries/queries.json",
            "block_selection_policy": "LatestBlocksFirst",
            "avg_num_tasks_per_block": 100
        }
    }
}