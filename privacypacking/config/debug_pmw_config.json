{
    "omegaconf": {
        "epsilon": 10,
        "delta": 1e-07,
        "enable_random_seed": true,
        "scheduler": {
            "metric_recomputation_period": 50,
            "scheduler_timeout_seconds": 1200,
            "data_lifetime": 0.1,
            "task_lifetime": 1,
            "planner": "PerBlockPlanner",
            "cache": "ProbabilisticCache",
            "cache_cfg": {
                "alpha": 0.005,
                "beta": 0.0001
            },
            "scheduling_wait_time": 1,
            "method": "batch",
            "metric": "Fcfs",
            "n": 1,
            "enable_caching": true,
            "enable_dp": true
        },
        "metric": {
            "normalize_by": "available_budget",
            "n_knapsack_solvers": 1
        },
        "logs": {
            "verbose": false,
            "save": true,
            "mlflow": true
        },
        "blocks": {
            "initial_num": 2,
            "max_num": 2,
            "path": "covid19/covid19_data/blocks",
            "metadata": "covid19/covid19_data/metadata.json"
        },
        "tasks": {
            "sampling": "poisson",
            "path": "covid19/covid19_workload/2blocks_1000queries.privacy_tasks.csv",
            "queries_path": "covid19/covid19_queries/1000.queries.json",
            "block_selection_policy": "LatestBlocksFirst",
            "avg_num_tasks_per_block": 500,
            "max_num": 2000,
            "initial_num": 2000
        }
    }
}