{   
    "mock": true,
    "variance_reduction": true,

    "cache": {
        "host": "localhost",
        "port": 6378,
        "type": "CombinedCache",
        "probabilistic_cfg": {
            "alpha": 0.05,
            "beta": 0.0001,
            "max_pmw_k": 10
        },
        "pmw_cfg": {
            "heuristic": "total_updates_counts",
            "heuristic_value": 100
        }
    },

    "planner": {
        "method": "min_cuts"
    },

    "budget_accountant": {
        "host": "localhost",
        "port": 6379,
        "epsilon": 5,
        "delta": 1e-07
    },

    "postgres": {
        "host": "localhost",
        "port": 5432,
        "database": "covid",
        "username": "postgres",
        "password": "postgres"
    },

    "blocks": {
        "initial_num": 1,
        "max_num": 1,
        "arrival_interval": 1,
        "block_data_path": "data/covid19/covid19_data/blocks",
        "block_metadata_path": "data/covid19/covid19_data/blocks/metadata.json"
    },
    
    "tasks": {
        "path": "data/covid19/covid19_workload/1:1blocks_34425queries.privacy_tasks.csv",
        "avg_num_tasks_per_block": 1e2,
        "max_num": 1e2,
        "initial_num": 0,
        "zipf_k": 0.5
    },

    "blocks_server": {
        "host": "localhost",
        "port": 1025
    },

    "tasks_server": {
        "host": "localhost",
        "port": 1024
    },

    "logs": {
        "verbose": false,
        "save": true,
        "mlflow": true,
        "loguru_level": "INFO"
    }
}