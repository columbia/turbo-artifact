{   
    "mock": true,
    "enable_random_seed": true,
    "enable_caching": true,
    "enable_dp": true,

    "cache": {
        "host": "localhost",
        "port": 6378,
        "type": "DeterministicCache",
        "pmw_cfg": {
            "alpha": 0.05,
            "beta": 0.0001,
            "standart_svt": true,
            "output_counts": true
        }
    },

    "planner": {
        "method": "MinCutsPlanner",
        "branching_factor_constraint": 2
    },

    "budget_accountant": {
        "host": "localhost",
        "port": 6379,
        "epsilon": 10,
        "delta": 1e-07
    },

    "postgres": {
        "host": "localhost",
        "port": 5432,
        "database": "covid",
        "username": "postgres",
        "password": "postgres"
    },

    "blocks": {
        "initial_num": 1,
        "max_num": 10,
        "arrival_interval": 1,
        "block_data_path": "data/covid19/covid19_data/blocks",
        "block_metadata_path": "data/covid19/covid19_data/metadata.json"
    },
    
    "tasks": {
        "path": "data/covid19/covid19_workload/400:7blocks_84queries.privacy_tasks.csv",
        "queries_path": "data/covid19/covid19_queries/all_2way_marginals.queries.json",
        "avg_num_tasks_per_block": 1,
        "max_num": 15,
        "initial_num": 0
    },

    "blocks_server": {
        "host": "localhost",
        "port": 1025
    },

    "tasks_server": {
        "host": "localhost",
        "port": 1024
    },

    "logs": {
        "verbose": false,
        "save": true,
        "mlflow": true,
        "loguru_level": "INFO"
    }
}